{
  "manifest_version": 2,

  "name": "On the spot Google Map search",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
    "http://maps.google.com/maps",
    "contextMenus",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon_browser_action.png",
    "default_popup": "popup.html"
  },
  "icons":{
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "background": {
        "scripts": ["search.js"]
    }
}